Subject: [PATCH 037/448] sdk: dts: t104xrdb: add the fifth DPAA Ethernet port

Signed-off-by: Camelia Groza <camelia.groza@nxp.com>
---
 arch/powerpc/boot/dts/fsl/t1040d4rdb-sdk.dts |    4 ++++
 arch/powerpc/boot/dts/fsl/t1040rdb-sdk.dts   |    4 ++++
 arch/powerpc/boot/dts/fsl/t1042rdb-sdk.dts   |    4 ++++
 3 files changed, 12 insertions(+), 0 deletions(-)

diff --git a/arch/powerpc/boot/dts/fsl/t1040d4rdb-sdk.dts b/arch/powerpc/boot/dts/fsl/t1040d4rdb-sdk.dts
index 3771ae3..5f9fa79 100644
--- a/arch/powerpc/boot/dts/fsl/t1040d4rdb-sdk.dts
+++ b/arch/powerpc/boot/dts/fsl/t1040d4rdb-sdk.dts
@@ -23,5 +23,9 @@
 			compatible = "fsl,dpa-ethernet";
 			fsl,fman-mac = <&enet3>;
 		};
+		ethernet@4 {
+			compatible = "fsl,dpa-ethernet";
+			fsl,fman-mac = <&enet4>;
+		};
 	};
 };
